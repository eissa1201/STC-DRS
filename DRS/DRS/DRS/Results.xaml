<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DRS.Results" BackgroundImage="List.jpg">
  <StackLayout>


    <ListView x:Name="listView"  >
      <ListView.ItemTemplate >
        <DataTemplate >
          <ViewCell >
            <ViewCell.View >
              <Label  Text="{Binding}" HorizontalOptions="CenterAndExpand" TextColor="White" FontSize="25" />

            </ViewCell.View>
          </ViewCell>
        </DataTemplate>
      </ListView.ItemTemplate>
    </ListView>



  </StackLayout>

</ContentPage>